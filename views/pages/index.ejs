<% if (pages) {%>
<nav aria-label="Page navigation">
	<ul class="pagination">
<% if (!pages["first_page"]){%><li><a href="<%=pages["uri"]%>/" aria-label="первая"><span aria-hidden="true">&leftarrow;&nbsp;первая</span></a></li><%}%>
<% pages["links"].forEach(function (link)
{
	if (pages["page"] == link){%><li class="active"><span><%=link%></span></li><%}
	else {%><li><a href="<%=(link == 1 ? pages["uri"] : pages["uri"]+'/page/'+link)%>/"><%=link%></a></li><%}
});%>
<% if (!pages["last_page"]){%><li><a  href="<%=pages["uri"]+'/page/'+pages["total_pages"]%>/" aria-label="последняя"><span aria-hidden="true">последняя&nbsp;&rightarrow;</span></a></li><%}%>
	</ul>
</nav>
<% } %>